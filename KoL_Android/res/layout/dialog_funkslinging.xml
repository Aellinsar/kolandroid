    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.starfish.kol.android.dialogs.FunkslingingDialog" >

    <FrameLayout
        android:id="@+id/funksling_item1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    
    <FrameLayout
        android:id="@+id/funksling_item2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
	    
    <FrameLayout
        android:id="@+id/funksling_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>